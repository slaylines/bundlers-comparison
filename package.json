{
  "name": "bundlers-comparison",
  "version": "1.0.0",
  "description": "Performance and usage comparison of Webpack 4, Rollup, Parcel and FuseBox bundlers.",
  "main": "app/src/index.js",
  "repository": "https://github.com/tehcookies/bundlers-comparison.git",
  "author": "TehCookies <contact@tehcookies.com>",
  "license": "MIT",
  "scripts": {
    "webpack:serve": "webpack-dev-server --progress --colors --config ./webpack/development.js",
    "webpack:build": "NODE_ENV=production webpack -p --progress --colors --config ./webpack/production.js",
    "parcel:serve": "parcel serve app/src/index.html --open --out-dir ./dist/parcel",
    "parcel:build": "NODE_ENV=production parcel build app/src/index.html --out-dir ./dist/parcel --public-url ./",
    "rollup:serve": "rollup --watch --config ./rollup/development.js",
    "rollup:build": "NODE_ENV=production rollup --config ./rollup/production.js"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^2.0.1",
    "exports-loader": "^0.7.0",
    "imports-loader": "^0.8.0",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.10.3",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-progress": "^0.4.0",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-scss": "^0.4.0",
    "rollup-plugin-serve": "^0.6.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-plugin-visualizer": "^0.9.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "js-cookie": "2.1.0",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "normalizr": "2.0.0",
    "prop-types": "15.5.10",
    "react": "16.0.0",
    "react-clipboard.js": "1.1.2",
    "react-dom": "16.0.0",
    "react-rangeslider": "2.1.0",
    "react-redux": "5.0.6",
    "react-router": "4.1.1",
    "react-router-dom": "4.1.1",
    "react-tooltip": "3.3.0",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "whatwg-fetch": "2.0.3"
  }
}
